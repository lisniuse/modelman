{
  "name": "modelman",
  "version": "0.1.1",
  "description": "This is a generic model and validation library.",
  "license": "MIT",
  "homepage": "https://github.com/lisniuse/modelman",
  "repository": "https://github.com/lisniuse/modelman",
  "bugs": "https://github.com/lisniuse/modelman/issues",
  "keywords": [
    "model",
    "form",
    "validator",
    "schema"
  ],
  "authors": [
    "Zhibin Chen <17560235@qq.com> (https://github.com/lisniuse)"
  ],
  "main": "dist/cjs/modelman.js",
  "unpkg": "dist/umd/modelman.js",
  "module": "dist/es/modelman.js",
  "typings": "./index.d.ts",
  "files": [
    "dist",
    "src",
    "index.d.ts"
  ],
  "scripts": {
    "format": "prettier --write \"{src,test}/**/*.{js,ts}\" index.d.ts \"**/*.md\"",
    "format:check": "prettier --list-different \"{src,test}/**/*.{js,ts}\" index.d.ts \"**/*.md\"",
    "lint": "eslint src test",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm test -- --coverage",
    "build": "rollup -c"
  },
  "dependencies": {
    "ispro": "^0.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.12.1",
    "eslint-config-react-app": "^3.0.6",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "glob": "^7.1.3",
    "jest": "^24.0.0",
    "mongoose": "^5.4.19",
    "prettier": "^1.16.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.3",
    "rxjs": "^6.3.3",
    "typescript": "^3.2.4",
    "typings-tester": "^0.3.2"
  },
  "npmName": "modelman",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "jest": {
    "testRegex": "(/test/.*\\.spec\\.js)$"
  },
  "sideEffects": false
}
